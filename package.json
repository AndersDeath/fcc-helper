  "bugs": {
    "url": "https://github.com/AndersDeath/fcc-helper/issues"
  },
  "homepage": "https://github.com/AndersDeath/fcc-helper#readme",
  "devDependencies": {
    "copy-webpack-plugin": "^9.0.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  }
}
